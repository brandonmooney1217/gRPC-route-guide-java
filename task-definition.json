{
  "family": "routeguide-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::595188147821:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "routeguide-container",
      "image": "595188147821.dkr.ecr.us-east-1.amazonaws.com/routeguide-grpc:latest",
      "portMappings": [
        {
          "containerPort": 8980,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/routeguide",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
